{
    "compilerOptions": {
      // 解决prettier对于装饰器语法的警告
      "experimentalDecorators": true,
      "baseUrl": ".",
      "allowJs": true,
      "paths": {
        "@/*": ["src/*"]
      },
      // 解决.jsx文件无法快速跳转的问题
      "jsx": "preserve"
    },
    //提高 IDE 性能
    "exclude": ["node_modules", "dist"]
  }